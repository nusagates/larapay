{
    "name": "nusagates/larapay",
    "description": "Laravel helper for iPaymu integration",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.0.0"
    },
    "autoload": {
        "psr-4": {
            "Nusagates\\Larapay\\": "src/"
        }
    },
    "authors": [
        {
            "name": "nusagates",
            "email": "nusagates@gmail.com"
        }
    ],
    "keywords": [
        "laravel payment gateway integration",
        "payment gateway",
        "indonesia payment gateway"
    ],
    "extra": {
