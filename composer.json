            "dev-master": "0.0.1-dev"
        },
        "laravel": {
            "dont-discover": [],
            "providers": [
                "Nusagates\\Larapay\\Providers\\LarapayProvider"
            ],
            "aliases": {
                "Ipaymu": "Nusagates\\Larapay\\Facade"
            }
        }
    }
